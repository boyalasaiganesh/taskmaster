{
    "family": "taskmaster-app",
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
      {
        "name": "taskmaster-container",
        "image": "008971673921.dkr.ecr.us-east-1.amazonaws.com/taskmaster-app:latest",
        "memory": 512,
        "cpu": 256,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "SPRING_PROFILES_ACTIVE",
            "value": "prod"
          }
        ]
      }
    ],
    "executionRoleArn": "arn:aws:iam::008971673921:role/ecsTaskExecutionRole"
  }
  