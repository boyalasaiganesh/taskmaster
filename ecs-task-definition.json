{
    "containerDefinitions": [
        {
            "name": "taskmaster-container",
            "image": "your-image-url",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "essential": true
        }
    ],
    "family": "taskmaster",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::008971673921:role/ecsTaskExecutionRole"
}
